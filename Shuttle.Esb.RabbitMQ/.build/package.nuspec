<?xml version="1.0"?>

<package>
  <metadata>
    <id>Shuttle.Esb.RabbitMQ</id>
    <version>{semver}</version>
    <authors>Eben Roux</authors>
    <owners>Eben Roux</owners>
    <license type="expression">BSD-3-Clause</license>
    <projectUrl>https://github.com/shuttle/Shuttle.Esb.RabbitMQ</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/Shuttle/Shuttle.Esb.RabbitMQ/master/.media/logo.png</iconUrl>
    <repository type="git" url="https://github.com/Shuttle/Shuttle.Esb.RabbitMQ.git" />
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>RabbitMQ implementation for use with Shuttle.Esb.</description>
    <releaseNotes />
    <copyright>Copyright (c) {year}, Eben Roux</copyright>
    <tags>shuttle rabbitmq</tags>
    <dependencies>
      <group>
        <dependency id="RabbitMQ.Client" version="{RabbitMQ.Client-version}" />
        <dependency id="Shuttle.Esb" version="{Shuttle.Esb-version}" />
        <dependency id="Shuttle.Core.Container" version="{Shuttle.Core.Container-version}" />
        <dependency id="Shuttle.Core.Uris" version="{Shuttle.Core.Uris-version}" />
      </group>

      <group targetFramework="netstandard2.0">
        <dependency id="RabbitMQ.Client" version="{RabbitMQ.Client-version}" />
        <dependency id="Shuttle.Esb" version="{Shuttle.Esb-version}" />
        <dependency id="Shuttle.Core.Container" version="{Shuttle.Core.Container-version}" />
        <dependency id="Shuttle.Core.Uris" version="{Shuttle.Core.Uris-version}" />
        <dependency id="System.Configuration.ConfigurationManager" version="{System.Configuration.ConfigurationManager-version}" />
      </group>

      <group targetFramework="netcoreapp2.1">
        <dependency id="RabbitMQ.Client" version="{RabbitMQ.Client-version}" />
        <dependency id="Shuttle.Esb" version="{Shuttle.Esb-version}" />
        <dependency id="Shuttle.Core.Container" version="{Shuttle.Core.Container-version}" />
        <dependency id="Shuttle.Core.Uris" version="{Shuttle.Core.Uris-version}" />
        <dependency id="System.Configuration.ConfigurationManager" version="{System.Configuration.ConfigurationManager-version}" />
      </group>
    </dependencies>
  </metadata>
</package>